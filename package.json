{
  "name": "playwright-sso-test",
  "version": "1.0.0",
  "description": "Playwright framework for testing SSO-protected pages",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:serial": "playwright test --workers=1",
    "test:debug": "playwright test --debug",
    "test:action-ecom": "playwright test --project=action-ecom-setup --project=action-ecom-chromium",
    "test:action-ecom:headed": "playwright test --project=action-ecom-setup --project=action-ecom-chromium --headed",
    "test:action-ecom:serial": "playwright test --project=action-ecom-setup --project=action-ecom-chromium --workers=1",
    "auth:setup:action-ecom": "playwright test --project=action-ecom-setup --headed",
    "test:stg-rijn-ijssel": "playwright test --project=stg-rijn-ijssel-setup --project=stg-rijn-ijssel-chromium",
    "test:stg-rijn-ijssel:headed": "playwright test --project=stg-rijn-ijssel-setup --project=stg-rijn-ijssel-chromium --headed",
    "test:stg-rijn-ijssel:serial": "playwright test --project=stg-rijn-ijssel-setup --project=stg-rijn-ijssel-chromium --workers=1",
    "auth:setup:stg-rijn-ijssel": "playwright test --project=stg-rijn-ijssel-setup --headed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testomatio/reporter": "^2.3.9",
    "@types/node": "^20.10.0",
    "dotenv": "^16.3.1"
  }
}
